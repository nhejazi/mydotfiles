"*":
  "autocomplete-plus":
    confirmCompletion: "tab"
    defaultProvider: "Fuzzy"
  core:
    audioBeep: false
    disabledPackages: [
      "linter-lintr"
    ]
    followSymlinks: true
    ignoredNames: [
      ".git"
      ".hg"
      ".svn"
      ".DS_Store"
      "*.pyc"
    ]
    openEmptyEditorOnStart: false
    telemetryConsent: "limited"
    themes: [
      "seti-ui"
      "seti-syntax"
    ]
  editor:
    fontSize: 13
    invisibles:
      cr: "↩"
      eol: " "
      tab: "⇥"
    preferredLineLength: 80
    scrollPastEnd: true
    showIndentGuide: true
    showInvisibles: false
    softWrap: true
    softWrapAtPreferredLineLength: true
    tabType: "soft"
  "exception-reporting":
    userId: "ceb72210-3bbf-ef71-d8fd-ac9521b8fc8b"
  "file-icons":
    coloured: false
  "highlight-selected":
    showInStatusBar: false
  "julia-client":
    enableMenu: true
    enableToolBar: true
    launchOnStartup: false
  linter:
    showErrorPanel: false
  minimap:
    plugins:
      cursorline: true
      cursorlineDecorationsZIndex: 0
      "find-and-replace": true
      "find-and-replaceDecorationsZIndex": 0
      "git-diff": true
      "git-diffDecorationsZIndex": 0
      "highlight-selected": true
      "highlight-selectedDecorationsZIndex": 0
      pigments: true
      pigmentsDecorationsZIndex: 0
      selection: true
      selectionDecorationsZIndex: 0
  "r-exec":
    advancePosition: false
    focusWindow: true
    notifications: true
    whichApp: "Google Chrome"
  "split-diff":
    diffWords: true
    ignoreWhitespace: true
    leftEditorColor: "red"
    rightEditorColor: "green"
    syncHorizontalScroll: true
  "tool-bar":
    position: "Left"
  "tree-view":
    hideVcsIgnoredFiles: true
  "vim-mode": {}
  "uber-juno":
    disable: true
  welcome:
    showOnStartup: false
